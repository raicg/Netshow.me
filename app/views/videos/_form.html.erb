
<%= simple_form_for(@video) do |f| %>
  <%= render "shared/error_messages", resource: @video %>

  <div class="form-inputs">
  <%= f.input :name %>
  <br/>
  <%= f.input :url %>
  </div>

  <br/>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
